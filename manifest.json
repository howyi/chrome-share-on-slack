{
	"manifest_version": 2,

	"name": "Share on Slack",
	"short_name": "Slack",
	"description": "Browse URL send to Slack channel.",
	"version": "1.0.0",
	"permissions": [
		"tabs"
	],
	"icons": {
		"128": "icon.png"
	},
	 "browser_action": {
		"default_icon": "share_icon.png",
		"default_popup": "popup.html"
	},
	"options_page": "options.html"
}
